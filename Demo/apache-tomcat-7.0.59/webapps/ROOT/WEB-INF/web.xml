<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>HDPAppStudio</display-name>
	<servlet>
		<description></description>
		<display-name>AppStudioDataSearcher</display-name>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hdpappstudio.web.AppStudioDataSearcher</servlet-class>
		<init-param>
			<param-name>hbasetable</param-name>
<param-value>hr</param-value>
		</init-param>
		<init-param>
			<param-name>hbasecolumnfamily</param-name>
<param-value>all</param-value>
		</init-param>
		<init-param>
			<param-name>solrurl</param-name>
<param-value>http://sandbox:8983/solr/hr</param-value>
		</init-param>
		<init-param>
			<param-name>pivotfield</param-name>
<param-value>heartrate</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/searchLocations</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/hbaseLocations</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/hbase</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/solrData</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AppStudioDataSearcher</servlet-name>
		<url-pattern>/data/hbaseData</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>HortonsGym</display-name>
		<servlet-name>HortonsGym</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hortonsgym.HortonsGym</servlet-class>
		<init-param>
			<param-name>brokerList</param-name>
<param-value>sandbox:6667</param-value>
		</init-param>
		<init-param>
			<param-name>acks</param-name>
			<param-value>1</param-value>
		</init-param>
		<init-param>
			<param-name>partitioner</param-name>
			<param-value>kafka.producer.DefaultPartitioner</param-value>
		</init-param>
		<init-param>
			<param-name>serializer</param-name>
			<param-value>kafka.serializer.StringEncoder</param-value>
		</init-param>
		<init-param>
			<param-name>topic</param-name>
<param-value>hr</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/sessions/running</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/hr/sessions/running</url-pattern>
	</servlet-mapping>
 <servlet-mapping>
                <servlet-name>HortonsGym</servlet-name>
                <url-pattern>/data/publish</url-pattern>
        </servlet-mapping>
 <servlet-mapping>
                <servlet-name>HortonsGym</servlet-name>
                <url-pattern>/data/upload</url-pattern>
        </servlet-mapping>
<servlet>
                <description></description>
                <display-name>HortonsGymModel</display-name>
                <servlet-name>Model</servlet-name>
                <servlet-class>com.hortonworks.digitalemil.hortonsgym.ModelUpdater</servlet-class>
                <init-param>
                        <param-name>zookeeper</param-name>
                        <param-value>sandbox:2181</param-value>
                </init-param>
        </servlet>
        <servlet-mapping>
                <servlet-name>Model</servlet-name>
                <url-pattern>/model/view</url-pattern>
        </servlet-mapping>
       <servlet-mapping>
                <servlet-name>Model</servlet-name>
                <url-pattern>/model/update</url-pattern>
        </servlet-mapping>
</web-app>

