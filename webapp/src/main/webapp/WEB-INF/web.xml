<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Horton's Gym</display-name>
	<servlet>
		<description></description>
		<display-name>HortonsGym</display-name>
		<servlet-name>HortonsGym</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hortonsgym.HortonsGym</servlet-class>
		<init-param>
			<param-name>onGoogleAppEngine</param-name>
			<param-value>true</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/sessions/running</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/hr/sessions/running</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/data/publish</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/data/upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/safemode</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/savemode</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/safe</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/data/pull</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>HortonsGym</servlet-name>
		<url-pattern>/data/color</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>HortonsGymModel</display-name>
		<servlet-name>Model</servlet-name>
		<servlet-class>com.hortonworks.digitalemil.hortonsgym.ModelUpdater</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Model</servlet-name>
		<url-pattern>/model/view</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Model</servlet-name>
		<url-pattern>/model/update</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Model</servlet-name>
		<url-pattern>/model/pmml</url-pattern>
	</servlet-mapping>
</web-app>

